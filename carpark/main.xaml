            <x:String>0 - 2 Hours = £2.00</x:String>
            <x:String>3 - 5 Hours = £6.00</x:String>
            <x:String>6 - 8 Hours = £10.00</x:String>
            <x:String>8 Hours+ = £15.00</x:String>
            <TextBlock Height="36" Text="TextBlock" TextWrapping="Wrap" Width="102"/>
        </ListView>
        <Button x:Name="btn_enterACar1" Content="Enter A Car" HorizontalAlignment="Left" Height="45" Margin="20,92,0,0" VerticalAlignment="Top" Width="142" FontSize="18" Click="btn_enterACar1_Click"/>
