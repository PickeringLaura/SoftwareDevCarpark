        <TextBlock x:Name="txt_space3" HorizontalAlignment="Left" Height="31" Margin="201,341,0,0" Text="Space 3" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.48,0.71"/>
        <TextBlock x:Name="txt_space4" HorizontalAlignment="Left" Height="31" Margin="60,380,0,0" Text="Space 4" TextWrapping="Wrap" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="txt_space5" HorizontalAlignment="Left" Height="31" Margin="143,377,0,0" Text="Space 5" TextWrapping="Wrap" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="txt_spacesAvailable1" HorizontalAlignment="Left" Height="62" Margin="26,45,0,0" Text="#AvailableSpaces#" TextWrapping="Wrap" VerticalAlignment="Top" Width="204" FontSize="24"/>
