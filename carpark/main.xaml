        <TextBlock x:Name="txt_Payment1" HorizontalAlignment="Left" Height="41" Margin="1155,48,0,0" Text="Payment" TextWrapping="Wrap" VerticalAlignment="Top" Width="99" FontSize="18"/>
        <TextBlock x:Name="txt_AmountDue1" HorizontalAlignment="Left" Height="40" Margin="1147,89,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="136" FontSize="18"/>
        <PasswordBox x:Name="pass_enterDiscount1" HorizontalAlignment="Left" Height="36" Margin="1144,129,0,0" VerticalAlignment="Top" Width="143" PlaceholderText="#EnterDiscount#" FontSize="16"/>
        <Button x:Name="btn_submit3" Content="Submit" HorizontalAlignment="Left" Height="34" Margin="1295,131,0,0" VerticalAlignment="Top" Width="82" FontSize="16" Click="btn_submit3_Click"/>
        <TextBlock x:Name="txt_selectPayment1" HorizontalAlignment="Left" Height="30" Margin="1143,180,0,0" Text="Select Payment Method" TextWrapping="Wrap" VerticalAlignment="Top" Width="237" FontSize="18"/>
        <Button x:Name="btn_emergencyTheft" Content="Theft" HorizontalAlignment="Left" Height="58" Margin="962,224,0,0" VerticalAlignment="Top" Width="127" Click="btn_emergencyTheft_Click"/>
        <Button x:Name="btn_emergencyFire_Copy" Content="Fire" HorizontalAlignment="Left" Height="58" Margin="960,161,0,0" VerticalAlignment="Top" Width="129" Click="btn_emergencyFire_Copy_Click"/>
        <Button x:Name="btn_emergencyStop" Content="Stop" HorizontalAlignment="Left" Height="35" Margin="965,291,0,0" VerticalAlignment="Top" Width="123" Click="btn_emergencyStop_Click"/>
        <TextBlock x:Name="txt_Pay1" HorizontalAlignment="Left" Height="37" Margin="1302,41,0,0" Text="Pay 1" TextWrapping="Wrap" VerticalAlignment="Top" Width="80"/>
        <TextBlock x:Name="txt_Pay2" HorizontalAlignment="Left" Height="34" Margin="1389,41,0,0" Text="Pay 2" TextWrapping="Wrap" VerticalAlignment="Top" Width="78"/>
        <TextBlock x:Name="txt_pay3" HorizontalAlignment="Left" Height="34" Margin="1349,83,0,0" Text="Pay 3" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.393,0.162"/>
        <TextBlock x:Name="txt_paymentDone" HorizontalAlignment="Left" Height="93" Margin="969,366,0,0" Text="You Have Paid, Extra Through Exit #Exit#" TextWrapping="Wrap" VerticalAlignment="Top" Width="135"/>
