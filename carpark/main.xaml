            <ListView x:Name="Lstvw_Prices1" HorizontalAlignment="Left" Height="96" Margin="14,252,0,0" VerticalAlignment="Top" Width="247" SelectionChanged="Combobx_Prices_SelectionChanged">
                <x:String>0 - 2 Hours = £2.00</x:String>
                <x:String>3 - 5 Hours = £6.00</x:String>
                <x:String>6 - 8 Hours = £10.00</x:String>
                <x:String>8 Hours+ = £15.00</x:String>
            </ListView>
            <Button x:Name="btn_enterACar1" Content="Enter A Car" HorizontalAlignment="Left" Height="45" Margin="66,91,0,0" VerticalAlignment="Top" Width="142" FontSize="18" Click="btn_enterACar1_Click"/>
            <Button x:Name="btn_exitCar1" Content="Exit A Car" HorizontalAlignment="Left" Height="39" Margin="219,94,0,0" VerticalAlignment="Top" Width="142" FontSize="18" Click="btn_exitCar1_Click"/>
