            <TextBlock x:Name="txt_Payment1" HorizontalAlignment="Left" Height="41" Margin="1098,47,0,0" Text="Choose Pay Machine:" TextWrapping="Wrap" VerticalAlignment="Top" Width="187" FontSize="18"/>
            <TextBlock x:Name="txt_AmountDue1" HorizontalAlignment="Left" Height="33" Margin="1147,96,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="136" FontSize="18"/>
            <PasswordBox x:Name="pass_enterDiscount1" HorizontalAlignment="Left" Height="36" Margin="1144,129,0,0" VerticalAlignment="Top" Width="143" PlaceholderText="#EnterDiscount#" FontSize="16"/>
            <Button x:Name="btn_submit3" Content="Submit" HorizontalAlignment="Left" Height="34" Margin="1295,131,0,0" VerticalAlignment="Top" Width="82" FontSize="16" Click="btn_submit3_Click"/>
            <TextBlock x:Name="txt_selectPayment1" HorizontalAlignment="Left" Height="30" Margin="1143,180,0,0" Text="Select Payment Method" TextWrapping="Wrap" VerticalAlignment="Top" Width="237" FontSize="18"/>
            <Button x:Name="btn_emergencyTheft" Content="Theft" HorizontalAlignment="Left" Height="58" Margin="962,224,0,0" VerticalAlignment="Top" Width="127" Click="btn_emergencyTheft_Click"/>
            <Button x:Name="btn_emergencyFire_Copy" Content="Fire" HorizontalAlignment="Left" Height="58" Margin="960,161,0,0" VerticalAlignment="Top" Width="129" Click="btn_emergencyFire_Copy_Click"/>
            <Button x:Name="btn_emergencyStop" Content="Stop" HorizontalAlignment="Left" Height="35" Margin="965,291,0,0" VerticalAlignment="Top" Width="123" Click="btn_emergencyStop_Click"/>
            <TextBlock x:Name="txt_Pay1" HorizontalAlignment="Left" Height="37" Margin="1302,41,0,0" Text="Pay 1" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" TextAlignment="Center"/>
            <TextBlock x:Name="txt_Pay2" HorizontalAlignment="Left" Height="34" Margin="1389,41,0,0" Text="Pay 2" TextWrapping="Wrap" VerticalAlignment="Top" Width="78" TextAlignment="Center"/>
            <TextBlock x:Name="txt_pay3" HorizontalAlignment="Left" Height="34" Margin="1349,83,0,0" Text="Pay 3" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.393,0.162" TextAlignment="Center"/>
            <TextBlock x:Name="txt_paymentDone" HorizontalAlignment="Left" Height="93" Margin="969,366,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="135"/>
            <Image x:Name="pic_android" HorizontalAlignment="Left" Height="92" Margin="1140,234,0,0" VerticalAlignment="Top" Width="108" Source="Assets/androidpaylogo-outlined.png" Tapped="pic_android_Tapped"/>
            <Image x:Name ="pic_apple" HorizontalAlignment="Left" Height="62" Margin="1284,252,0,0" VerticalAlignment="Top" Width="108" Source="Assets/apple-pay-logo-F68C9AC252-seeklogo.com.png" Tapped="pic_apple_Tapped"/>
            <Image x:Name="pic_contactless" HorizontalAlignment="Left" Height="76" Margin="1150,344,0,0" VerticalAlignment="Top" Width="108" Source="Assets/Universal_Contactless_Card_Symbol.svg.png" RenderTransformOrigin="0.667,0.079" Tapped="pic_contactless_Tapped"/>
            <Image x:Name="pic_card" HorizontalAlignment="Left" Height="36" Margin="1288,360,0,0" VerticalAlignment="Top" Width="108" Source="Assets/Visa.png" Tapped="pic_card_Tapped"/>
            <ListView HorizontalAlignment="Left" Height="103" Margin="10,141,0,0" VerticalAlignment="Top" Width="251">
                <x:String>Carpark 2 - 30 Available Spaces</x:String>
                <x:String>Carpark 3 - 20 Available Spaces</x:String>
                <x:String>Carpark 4 - 35 Available Spaces</x:String>
            </ListView>
        </Grid>
    </Viewbox>
</Page>
