        <TextBlock x:Name="txt_coinDispensed_Copy6" HorizontalAlignment="Left" Height="51" Margin="353,328,0,0" Text="Coin Collected" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
        <PasswordBox x:Name="pass_enterReg_Copy6" HorizontalAlignment="Left" Height="33" Margin="455,330,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterReg#" FontSize="16"/>
        <PasswordBox x:Name="pass_enterPassword_Copy6" HorizontalAlignment="Left" Height="33" Margin="604,327,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterPassword#" FontSize="16"/>
        <Button x:Name="btn_submit_Copy6" Content="Submit" HorizontalAlignment="Left" Height="37" Margin="753,325,0,0" VerticalAlignment="Top" Width="85" FontSize="16" Click="btn_submit_Copy6_Click"/>
        <Button x:Name="btn_unlock1" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="834,129,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock1_Click"/>
        <Button x:Name="btn_unlock_Copy4" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="839,179,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock_Copy4_Click"/>
        <Button x:Name="btn_unlock_Copy5" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="843,231,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock_Copy5_Click"/>
        <Button x:Name="btn_unlock_Copy6" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="845,276,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock_Copy6_Click"/>
        <Button x:Name="btn_unlock_Copy7" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="845,324,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock_Copy7_Click"/>
