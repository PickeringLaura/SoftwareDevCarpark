            <TextBlock x:Name="txt_space2" HorizontalAlignment="Left" Height="31" Margin="114,362,0,0" Text="Space 2" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" Tapped="txt_space2_Tapped"/>
            <TextBlock x:Name="txt_space3" HorizontalAlignment="Left" Height="31" Margin="30,401,0,0" Text="Space 3" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.48,0.71" Tapped="txt_space3_Tapped"/>
            <TextBlock x:Name="txt_space4" HorizontalAlignment="Left" Height="31" Margin="115,397,0,0" Text="Space 4" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" Tapped="txt_space4_Tapped"/>
            <TextBlock x:Name="txt_space5" HorizontalAlignment="Left" Height="31" Margin="74,428,0,0" Text="Space 5" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" Tapped="txt_space5_Tapped"/>
            <TextBlock x:Name="txt_spacesAvailable1" HorizontalAlignment="Left" Height="62" Margin="26,45,0,0" Text="#AvailableSpaces#" TextWrapping="Wrap" VerticalAlignment="Top" Width="204" FontSize="24"/>
            <TextBlock x:Name="txt_coinDispensed1" HorizontalAlignment="Left" Height="51" Margin="353,128,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
            <PasswordBox x:Name="pass_enterReg1" HorizontalAlignment="Left" Height="33" Margin="456,134,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterReg#" FontSize="16" PasswordChanged="pass_enterReg1_PasswordChanged"/>
            <PasswordBox x:Name="pass_enterPassword1" HorizontalAlignment="Left" Height="33" Margin="600,133,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterPassword#" FontSize="16"/>
            <Button x:Name="btn_submit2" Content="Submit" HorizontalAlignment="Left" Height="37" Margin="746,131,0,0" VerticalAlignment="Top" Width="85" FontSize="16" Click="btn_submit2_Click"/>
            <TextBlock x:Name="txt_bay1coinDispensed1" HorizontalAlignment="Left" Height="51" Margin="352,179,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
            <PasswordBox x:Name="pass_bay1enterReg1" HorizontalAlignment="Left" Height="33" Margin="455,182,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterReg#" FontSize="16"/>
            <PasswordBox x:Name="pass_bay1enterPassword1" HorizontalAlignment="Left" Height="33" Margin="602,183,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterPassword#" FontSize="16"/>
            <Button x:Name="btn_bay1submit1" Content="Submit" HorizontalAlignment="Left" Height="37" Margin="749,181,0,0" VerticalAlignment="Top" Width="85" FontSize="16" Click="btn_bay1submit1_Click"/>
            <TextBlock x:Name="txt_coinDispensed_Copy4" HorizontalAlignment="Left" Height="51" Margin="354,229,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
            <PasswordBox x:Name="pass_enterReg_Copy4" HorizontalAlignment="Left" Height="33" Margin="455,232,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterReg#" FontSize="16"/>
            <PasswordBox x:Name="pass_enterPassword_Copy4" HorizontalAlignment="Left" Height="33" Margin="605,230,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterPassword#" FontSize="16"/>
            <Button x:Name="btn_submit_Copy4" Content="Submit" HorizontalAlignment="Left" Height="37" Margin="752,231,0,0" VerticalAlignment="Top" Width="85" FontSize="16" Click="btn_submit_Copy4_Click"/>
            <TextBlock x:Name="txt_coinDispensed_Copy5" HorizontalAlignment="Left" Height="51" Margin="354,279,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
            <PasswordBox x:Name="pass_enterReg_Copy5" HorizontalAlignment="Left" Height="33" Margin="455,283,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterReg#" FontSize="16"/>
            <PasswordBox x:Name="pass_enterPassword_Copy5" HorizontalAlignment="Left" Height="33" Margin="604,277,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterPassword#" FontSize="16"/>
            <Button x:Name="btn_submit_Copy5" Content="Submit" HorizontalAlignment="Left" Height="37" Margin="754,275,0,0" VerticalAlignment="Top" Width="85" FontSize="16" Click="btn_submit_Copy5_Click"/>
            <TextBlock x:Name="txt_coinDispensed_Copy6" HorizontalAlignment="Left" Height="51" Margin="353,328,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
            <PasswordBox x:Name="pass_enterReg_Copy6" HorizontalAlignment="Left" Height="33" Margin="455,330,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterReg#" FontSize="16"/>
            <PasswordBox x:Name="pass_enterPassword_Copy6" HorizontalAlignment="Left" Height="33" Margin="604,327,0,0" VerticalAlignment="Top" Width="137" PlaceholderText="#EnterPassword#" FontSize="16"/>
            <Button x:Name="btn_submit_Copy6" Content="Submit" HorizontalAlignment="Left" Height="37" Margin="753,325,0,0" VerticalAlignment="Top" Width="85" FontSize="16" Click="btn_submit_Copy6_Click"/>
            <Button x:Name="btn_unlock1" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="834,129,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock1_Click"/>
            <Button x:Name="btn_unlock_Copy4" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="839,179,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock_Copy4_Click"/>
            <Button x:Name="btn_unlock_Copy5" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="843,231,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock_Copy5_Click"/>
            <Button x:Name="btn_unlock_Copy6" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="845,276,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock_Copy6_Click"/>
            <Button x:Name="btn_unlock_Copy7" Content="Unlock" HorizontalAlignment="Left" Height="39" Margin="845,324,0,0" VerticalAlignment="Top" Width="89" FontSize="18" Click="btn_unlock_Copy7_Click"/>
